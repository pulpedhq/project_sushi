<h1>Listing briefs</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Name</th>
    <th>File url</th>
    <th>Project</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @briefs.each do |brief| %>
  <tr>
    <td><%= brief.id %></td>
    <td><%= brief.name %></td>
    <td><%= brief.file_url %></td>
    <td><%= brief.project_id %></td>
    <td><%= link_to 'Show', brief %></td>
    <td><%= link_to 'Edit', edit_brief_path(brief) %></td>
    <td><%= link_to 'Destroy', brief, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Brief', new_brief_path %>
